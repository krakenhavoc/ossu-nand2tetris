/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel , out=notsel);
    And(a=a, b=notsel, out=andanotsel);
    And(a=b, b=sel, out=andbsel);
    Or(a=andanotsel, b=andbsel, out=out);
}